{
  "version": 2,
  "framework": "angular",
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist/frontend/browser",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/assets/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "/(.*\\.js)",
      "headers": {
        "content-type": "application/javascript"
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 