{
  "version": 2,
  "framework": "angular",
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist/frontend/browser",
  "routes": [
    {
      "src": "^/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "^/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "^/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "^/ngsw-worker.js",
      "dest": "/ngsw-worker.js"
    },
    {
      "src": "^/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|json))$",
      "dest": "/$1"
    },
    {
      "src": "^/(.*)",
      "dest": "/index.html"
    }
  ]
} 